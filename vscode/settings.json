{
    "window.commandCenter": 1,
    "editor.tabSize": 2,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "cursor.composer.shouldAutoScrollToBottom": false,
    "cursor.composer.agentLoopOnLints": false,
    "editor.fontFamily": "'Courier New', monospace",
    "editor.detectIndentation": false,
    "editor.renderWhitespace": "all",
    "editor.accessibilitySupport": "off",
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingDelete": "never",
    "editor.bracketPairColorization.enabled": false,
    "editor.codeLens": false,
    "editor.folding": false,
    "editor.foldingHighlight": false,
    "editor.gotoLocation.alternativeDeclarationCommand": "editor.action.goToImplementation",
    "editor.gotoLocation.alternativeDefinitionCommand": "editor.action.goToTypeDefinition",
    "editor.gotoLocation.alternativeTypeDefinitionCommand": "editor.action.goToTypeDefinition",
    "editor.guides.bracketPairs": true,
    "editor.hover.delay": 200,
    "editor.hover.hidingDelay": 500,
    "editor.inlayHints.enabled": "offUnlessPressed",
    "editor.mouseWheelZoom": true,
    "editor.occurrencesHighlight": "multiFile",
    "editor.rename.enablePreview": false,
    "editor.renderLineHighlightOnlyWhenFocus": true,
    "editor.roundedSelection": false,
    "editor.scrollbar.horizontal": "visible",
